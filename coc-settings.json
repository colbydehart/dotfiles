{
  "diagnostic.enableMessage": "jump",
  "list.source.files.command": "fzf",
  "list.source.grep.command": "rg",
  "list.insertMappings": {
    "<C-n>": "normal:j",
    "<C-p>": "normal:k",
    "<Space>": "feedkeys:<SPACE>",
  },
  "coc.preferences.formatOnSaveFiletypes": [
    "css",
    "javascript",
    "javascriptreact",
    "javascript.jsx",
    "typescript",
    "typescriptreact",
    "markdown",
    "json",
    "elixir",
    "eelixir"
  ],
  "tsserver.formatOnType": false,
  "tslint.enable": true,
  "languageserver": {
     "elixirLs": {
       "command": "/Users/colbydehart/.config/nvim/elixir-ls/release/language_server.sh",
       "filetypes": ["elixir", "eelixir"]
     },
    "reason": {
      "command": "/Users/colbydehart/.config/nvim/reason-language-server/reason-language-server.exe",
      "filetypes": ["reason"]
    }
  }
}
