{
  "diagnostic.virtualText": true,
  "diagnostic.messageTarget": "echo",
  "coc.source.around.priority": 9,
  "coc.source.buffer.priority": 9,
  "eslint.options": {"configFile": "./.eslintrc"},
  "json.format.enable": false,
  "prettier.requireConfig": true,
  "list.insertMappings": {
    "<C-n>": "normal:j",
    "<C-p>": "normal:k"
  },
  "coc.preferences.formatOnSaveFiletypes": [
    "css",
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "json"
  ],
  "eslint.filetypes": ["javascript", "javascriptreact", "typescript", "typescriptreact"],
  "tsserver.formatOnType": false,
  "languageserver": {
    "reason-language-server": {
      "command": "/Users/colbydehart/.config/nvim/reason-language-server/reason-language-server.exe",
      "filetypes": ["reason"]
    },
    "elixirLS": {
      "command": "/Users/colbydehart/.config/nvim/elixir-ls/language_server.sh",
      "filetypes": ["elixir", "eelixir"],
      "settings": { "dialyzerEnabled": false}
    },
    "terraform-lsp": {
      "command": "/Users/colbydehart/.config/nvim/terraform-lsp",
      "filetypes": ["terraform"],
      "initializationOptions": {}
    }
  }
}
