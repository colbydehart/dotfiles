{
  "editor.fontSize": 16,
  "editor.fontFamily": "Fira Code",
  "editor.tabSize": 2,
  "vim.neovimPath": "/usr/local/bin/nvim",
  "vim.enableNeovim": true,
  "editor.formatOnSave": true,
  "emmet.includeLanguages": {
    "javascript": "jsx",
    "reason": "javascriptreact"
  },
  "vim.handleKeys": {
    "<C-f>": false,
    "<C-a>": false,
    "<C-y>": true,
    "<C-n>": true
  },
  "editor.minimap.enabled": false,
  "terminal.integrated.fontSize": 16,
  "terminal.integrated.fontFamily": "Fira Code",
  "window.zoomLevel": 0,
  "vim.easymotion": true,
  "vim.leader": "space",
  "vim.easymotionMarkerFontFamily": "Inconsolata for Powerline",
  "vim.easymotionMarkerHeight": 25,
  "vim.easymotionMarkerFontSize": "16",
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["backspace"],
      "commands": [
        {
          "command": "workbench.action.openPreviousRecentlyUsedEditor",
          "args": []
        }
      ]
    },
    {
      "before": ["H"],
      "commands": [
        {
          "command": "workbench.action.previousEditor",
          "args": []
        }
      ]
    },
    {
      "before": ["L"],
      "commands": [
        {
          "command": "workbench.action.nextEditor",
          "args": []
        }
      ]
    },
    {
      "before": ["K"],
      "commands": [
        {
          "command": "editor.action.showHover",
          "args": []
        }
      ]
    },
    {
      "before": ["<C-h>"],
      "commands": [
        {
          "command": "workbench.action.focusLeftGroup",
          "args": []
        }
      ]
    },
    {
      "before": ["<C-l>"],
      "commands": [
        {
          "command": "workbench.action.focusRightGroup",
          "args": []
        }
      ]
    },
    {
      "before": ["<C-k>"],
      "commands": [
        {
          "command": "workbench.action.focusAboveGroup",
          "args": []
        }
      ]
    },
    {
      "before": ["<C-j>"],
      "commands": [
        {
          "command": "workbench.action.focusAboveGroup",
          "args": []
        }
      ]
    },
    {
      "before": ["<C-w>", "<C-w>"],
      "commands": [
        {
          "command": "workbench.action.quickSwitchWindow",
          "args": []
        }
      ]
    },
    {
      "before": ["<leader>", "<CR>"],
      "commands": [
        {
          "command": "workbench.action.showCommands",
          "args": []
        }
      ]
    },
    {
      "before": ["<leader>", "d"],
      "commands": [
        {
          "command": "workbench.view.explorer",
          "args": []
        }
      ]
    },
    {
      "before": ["<leader>", "e"],
      "commands": [
        {
          "command": "extension.advancedNewFile",
          "args": []
        }
      ]
    },
    {
      "before": ["<leader>", "f"],
      "commands": [
        {
          "command": "workbench.action.quickOpen",
          "args": []
        }
      ]
    },
    {
      "before": ["<leader>", "g"],
      "commands": [
        {
          "command": "workbench.view.scm",
          "args": []
        }
      ]
    },
    {
      "before": ["<leader>", "k"],
      "commands": [
        {
          "command": "workbench.action.closeActiveEditor",
          "args": []
        }
      ]
    },
    {
      "before": ["<leader>", "l"],
      "commands": [
        {
          "command": "workbench.action.showErrorsWarnings",
          "args": []
        }
      ]
    },
    {
      "before": ["<leader>", "o"],
      "commands": [
        {
          "command": "workbench.action.closeOtherEditors",
          "args": []
        }
      ]
    },
    {
      "before": ["<leader>", "p"],
      "commands": [
        {
          "command": "projectManager.listProjects",
          "args": []
        }
      ]
    },
    {
      "before": ["<leader>", "s", "h"],
      "commands": [
        {
          "command": "workbench.action.splitEditorLeft",
          "args": []
        }
      ]
    },
    {
      "before": ["<leader>", "s", "k"],
      "commands": [
        {
          "command": "workbench.action.splitEditorUp",
          "args": []
        }
      ]
    },
    {
      "before": ["<leader>", "s", "j"],
      "commands": [
        {
          "command": "workbench.action.splitEditorDown",
          "args": []
        }
      ]
    },
    {
      "before": ["<leader>", "s", "l"],
      "commands": [
        {
          "command": "workbench.action.splitEditorRight",
          "args": []
        }
      ]
    },
    {
      "before": ["<leader>", "t"],
      "commands": [
        {
          "command": "workbench.action.terminal.toggleTerminal",
          "args": []
        }
      ]
    },
    {
      "before": ["<leader>", "w"],
      "commands": [
        {
          "command": "workbench.action.files.save"
        }
      ]
    },
    {
      "before": ["<leader>", "y"],
      "commands": [
        {
          "command": "workbench.action.openSnippets",
          "args": []
        }
      ]
    },
    {
      "before": ["<leader>", "/"],
      "commands": [
        {
          "command": "workbench.action.findInFiles",
          "args": []
        }
      ]
    }
  ],
  "javascript.validate.enable": false,
  "workbench.iconTheme": "material-icon-theme",
  "git.autofetch": true,
  "prettier.trailingComma": "all",
  "workbench.sideBar.location": "right",
  "editor.lineNumbers": "relative",
  "csharp.referencesCodeLens.enabled": false,
  "liveshare.audio.joinCallBehavior": "accept",
  "liveshare.audio.startCallOnShare": true,
  "files.exclude": {
    "**/.shadow-cljs": true,
    "**/nashhorn_code_cache": true
  },
  "explorer.autoReveal": false,
  "liveshare.featureSet": "stable",
  "elixirLS.dialyzerEnabled": false
}
